@model IEnumerable<MVCIdentitySeedingLessson.Models.Book>

<h2>Список книг</h2>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Название</th>
            <th>Автор</th>
            <th>Дата публикации</th>
            <th>Жанр</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var book in Model)
        {
                <tr>
                    <td>@book.Title</td>
                    <td>@book.Author</td>
                    <td>@book.Year</td>
                    <td>@book.Genre</td>
                </tr>
        }
    </tbody>
</table>

